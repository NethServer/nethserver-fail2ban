{
    # Migrate BanAction to iptables-ipset-proto6-allports
    use esmith::ConfigDB;
    my $db = esmith::ConfigDB->open;

    my $BanAction = $db->get_prop('fail2ban','BanAction') || '';
    $db->set_prop('fail2ban','BanAction','iptables-ipset-proto6-allports') if ($BanAction eq 'shorewall-nethserver');
    '';
}
